import Foundation

public extension Resource {
    enum Localizable { }
}

public extension Resource.Localizable {
    <%- resources.each do |resource| -%>
    static var <%= resource.key %>: String { "<%= resource.value %>".localized(in: .module) }
    <%- end -%>
}