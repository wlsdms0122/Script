import UIKit
import SwiftUI

public extension Resource {
    enum icon { }
}

public extension Resource.icon {
    <%- resources.each do |resource| -%>
    static var <%= resource.key %>: Resource.ImageResource { Resource.ImageResource(named: "<%= resource.value %>", in: .module).glyph() }
    <%- end -%>
}
